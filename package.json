{
    "name": "wb-course-mini-app",
    "version": "1.0.0",
    "description": "Telegram Mini App для курса по выходу на Wildberries",
    "main": "index.js",
    "scripts": {
      "start": "concurrently \"npm run server\" \"npm run client\"",
      "server": "cd server && npm run dev",
      "client": "cd client && npm start",
      "install-all": "npm install && cd client && npm install && cd ../server && npm install",
      "build": "cd client && npm run build"
    },
    "keywords": ["telegram", "mini-app", "wildberries", "course"],
    "author": "",
    "license": "ISC",
    "dependencies": {
      "concurrently": "^8.0.1"
    }
  }